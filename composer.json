{
    "name": "sendflare/sendflare-sdk-php",
    "description": "The SDK for sendflare service written in PHP",
    "type": "library",
    "keywords": [
        "sendflare",
        "email",
        "sdk",
        "php",
        "api"
    ],
    "homepage": "https://docs.sendflare.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Sendflare Team",
            "homepage": "https://sendflare.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sendflare\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sendflare\\SDK\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

